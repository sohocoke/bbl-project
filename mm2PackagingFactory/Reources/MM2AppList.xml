<MM2AppList>
    <!--Worxmail deployed to CH and SG -->
    <App name="WorxMail" platform="iOS"  policy="worxmail" InputFile="worxmail_1_5_3.ipa">
            <region name="ch">
                <!--Both a CH and EUR variant will be deployed to PROD -->
                <stage name="prod">
                    <output>
                        <policyOverride>worxmail_eur_prod</policyOverride>
                        <bundleSuffix>eur</bundleSuffix>
                    </output>
                    <output>
                        <policyOverride>worxmail_ch_prod</policyOverride>
                        <bundleSuffix>ch</bundleSuffix>
                    </output>
                </stage>
                <stage name="pta">
                    <output>
                        <policyOverride>worxmail_ch_prod</policyOverride>
                        <bundleSuffix>ch</bundleSuffix>
                    </output>
                </stage>
                <!--In ADPTA and ETIT, an updated version of the IPA will be used -->
                <stage name="adpta">
                    <output>
                        <policyOverride>worxmail_ch_prod</policyOverride>
                        <bundleSuffix>ch</bundleSuffix>
                        <InputFileOveride>worxmail_1_5_4.ipa</InputFileOveride>
                    </output>
                </stage>
                <stage name="etit">
                    <output>
                        <policyOverride>worxmail_ch_test</policyOverride>
                        <bundleSuffix>ch</bundleSuffix>
                        <InputFileOveride>worxmail_1_5_4.ipa</InputFileOveride>
                    </output>
                </stage>
            </region>
        <!--SG AppC will contain both a SG and a HongKong version of WorxMail 1815 -->
        <region name="sg">
            <stage name="prod">
                <output>
                    <policyOverride>worxmail_sg_prod</policyOverride>
                    <bundleSuffix>sg</bundleSuffix>
                </output>
                <output>
                    <policyOverride>worxmail_hkg_prod</policyOverride>
                    <bundleSuffix>hkg</bundleSuffix>
                </output>
            </stage>
        </region>
    </App>
    <!--Office Squared will be deployed to CH,GB,SG and US AppCs. Updated IPAs will be deployedto  CH ETIT and US POC -->
    <App name="Office2" platform="iOS"  policy="Office2"  InputFile="officeSquared_1_1_1.ipa">
        <InputFile>officeSquared_1_1_1.ipa</InputFile>
        <region name="ch">
            <stage name="prod">
            </stage>
            <stage name="pta">
            </stage>
            <stage name="etit">
                <output>
                    <policyOverride>Office2_test</policyOverride>
                    <InputFileOveride>officeSquared_1_1_2.ipa</InputFileOveride>
                </output>
            </stage>
        </region>
        <region name="gb">
            <stage name="prod">
            </stage>
        </region>
        <region name="sg">
            <stage name="prod">
            </stage>
        </region>
        <region name="us">
            <stage name="prod">
            </stage>
            <stage name="pta">
            </stage>
            <stage name="etit">
            </stage>
            <stage name="poc">
                <output>
                    <policyOverride>Office2_test</policyOverride>
                    <InputFileOveride>officeSquared_1_1_2.ipa</InputFileOveride>
                </output>
            </stage>
        </region>
    </App>
    <!--WorxWeb should have two versions in the AppCs used by developers. One will be named WorxWeb Test and will have a Certificate Label of Test -->
    <App name="WorxWeb" platform="iOS"  policy="WorxWeb" InputFile="worxweb_1_7_74.ipa">
        <region name="ch">
            <stage name="prod">
            </stage>
            <stage name="pta">
            </stage>
            <stage name="adpta">
                <output default="true">
                </output>
                <output>
                    <policyOverride>worxmail_test</policyOverride>
                    <bundleSuffix>Test</bundleSuffix>
                </output>
            </stage>
        </region>
        <region name="us">
            <stage name="prod">
            </stage>
            <stage name="pta">
            </stage>
            <!--the default=true is neeed to generate the WorxWeb app in Prod. Without this attribute only WorxWeb Test would be generated-->
            <stage name="etit">
                <output default="true">
                </output>
                <output>
                    <policyOverride>worxmail_test</policyOverride>
                    <bundleSuffix>Test</bundleSuffix>
                </output>
            <stage name="poc">
                <output default="true">
                </output>
                <output>
                    <policyOverride>worxmail_test</policyOverride>
                    <bundleSuffix>Test</bundleSuffix>
                </output>
            </stage>
        </region>
    </App>
</MM2AppList>